{
  "name": "cms",
  "description": "Payload project created from ts-todo template",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev:seed": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts PAYLOAD_DROP_DATABASE=true PAYLOAD_SEED=true nodemon",
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "dev:win": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc",
    "build": "yarn copyfiles && yarn build:payload && yarn build:server",
    "build:docker": "yarn build:payload && yarn build:server",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/client-ses": "^3.451.0",
    "@payloadcms/plugin-seo": "^1.0.10",
    "@sendgrid/mail": "^7.7.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "moment": "^2.29.4",
    "mui": "^0.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer-sendgrid": "^1.0.3",
    "payload": "^1.6.15",
    "payload-webp": "^1.1.6",
    "qs": "^6.11.2",
    "querystring": "^0.2.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/multer": "^1.4.10",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6",
    "shared-types": "*",
    "ts-node": "^9.1.1",
    "typescript": "^4.8.4"
  },
  "resolutions": {
    "sharp": "latest"
  }
}
